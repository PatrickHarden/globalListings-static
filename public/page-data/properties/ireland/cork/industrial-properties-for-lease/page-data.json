n easy access of the M8 Dublin Motorway."}],"Common.NumberOfBedrooms":0,"Common.ParentProperty":"IE-Plus-477443","Common.Photos":[{"Common.ImageCaption":"IMAGE 1 WITH CBRE LOGO","Common.ImageResources":[{"Common.Breakpoint":"original","Common.Image.Height":603,"Common.Image.Width":902,"Common.Resource.Uri":"/resources/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo.JPG","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo.JPG"},{"Common.Breakpoint":"small","Common.Image.Height":427,"Common.Image.Width":640,"Common.Resource.Uri":"/resources/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo_Photo_1_small.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo.JPG"},{"Common.Breakpoint":"medium","Common.Image.Height":855,"Common.Image.Width":1280,"Common.Resource.Uri":"/resources/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo_Photo_1_medium.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo.JPG"},{"Common.Breakpoint":"large","Common.Image.Height":1069,"Common.Image.Width":1600,"Common.Resource.Uri":"/resources/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo_Photo_1_large.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/IE-Plus-477443-908349/bd84f0fd/Image%201%20with%20CBRE%20logo.JPG"}],"Common.AddWatermark":false},{"Common.ImageCaption":"IMAGE 2 WITH CBRE LOGO","Common.ImageResources":[{"Common.Breakpoint":"original","Common.Image.Height":597,"Common.Image.Width":891,"Common.Resource.Uri":"/resources/fileassets/IE-Plus-477443-908349/0b55ff20/Image%202%20with%20CBRE%20logo.JPG","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/IE-Plus-4