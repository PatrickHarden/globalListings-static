.\n"}],"Common.NumberOfBedrooms":0,"Common.NumberOfLots":0,"Common.Photos":[{"Common.ImageCaption":"Foster Plaza 3","Common.AddWatermark":false,"Common.ImageResources":[{"Common.Breakpoint":"original","Common.Image.Height":0,"Common.Image.Width":0,"Common.Resource.Uri":"/resources/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2.jpg"},{"Common.Breakpoint":"small","Common.Image.Height":400,"Common.Image.Width":640,"Common.Resource.Uri":"/resources/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2_Photo_1_small.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2.jpg"},{"Common.Breakpoint":"medium","Common.Image.Height":800,"Common.Image.Width":1280,"Common.Resource.Uri":"/resources/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2_Photo_1_medium.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2.jpg"},{"Common.Breakpoint":"large","Common.Image.Height":1000,"Common.Image.Width":1600,"Common.Resource.Uri":"/resources/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2_Photo_1_large.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/US-SMPL-19940/f05f5768/58059282-054f-4843-ac37-b8fc0c97cba2.jpg"}]},{"Common.ImageCaption":"IMG_1","Common.AddWatermark":false,"Common.ImageResources":[{"Common.Breakpoint":"original","Common.Image.Height":0,"Common.Image.Width":0,"Common.Resource.Uri":"/resources/fileassets/US-SMPL-19940/a191b0c9/c518d9b9-1428-4ead-8d3f-c9cb0eca3c46.jpg","Source.Uri":"https://wwwlistingssearchcbreeun.blob.core.windows.net/fileassets/US-SMPL-19940/a191b0c9/c518d9b9-1428-4ead-8d3f-c9cb0eca3c46.jpg"},{"Common.Breakpoint"